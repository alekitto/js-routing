{
    "name": "kcs/js-routing",
    "description": "Symfony's UrlGenerator for javascript with routes extractor and dumper",
    "license": "MIT",
    "authors": [
        {
            "name": "Alessandro Chitolina",
            "email": "alekitto@gmail.com"
        }
    ],
    "type": "library",
    "require": {
        "php": "^7.0",
        "symfony/routing": "^2.7|^3.0|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "symfony/config": "^2.7|^3.0|^4.0",
        "symfony/console": "^2.7|^3.0|^4.0",
        "symfony/dependency-injection": "^2.7|^3.0|^4.0",
        "symfony/framework-bundle": "^2.7|^3.0|^4.0",
        "symfony/twig-bundle": "^2.7|^3.0|^4.0",
        "symfony/yaml": "^2.7|^3.3|^4.0"
    },
    "conflict": {
        "twig/twig": "<1.34"
    },
    "autoload": {
        "psr-4": {
            "Kcs\\JsRouting\\": "src/",
            "Kcs\\JsRouting\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
